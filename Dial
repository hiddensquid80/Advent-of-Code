input = open('input_nums.txt','r')
# open file in read mode

action_list = input.read()
# each line is now a value in action_list

actions = action_list.split()
# now there is a list of each action to be taken

def take_actions(index):
    global start
    if 'L' in index:
        start -= int(index.split('L')[1])
    else:
        start += int(index.split('R')[1])

def in_range():
    global start
    while start not in range(0,100):
        if start > 99:
            start = start - 100
        elif start < 0:
            start = 100 + start

count = 0
start = 50
processed = 0
for action in actions:

    take_actions(action)

    in_range()

    if start == 0:
        count += 1

    processed += 1
print(count)
print(processed)